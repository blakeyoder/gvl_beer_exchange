   <div id="header">
    <h1>News/Events</h1>
    <div class="row">
      <% @events.each do |event| %>
          <div class="col-md-4 col-xs-12 event-box">
              <h2><%= event["name"] %></h2>
              <p class="events_description"><%= event["description"] %></p>
          </div>
      <% end %>
    </div>
  </div>
